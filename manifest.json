{
  "manifest_version": 3,
  "name": "Télécharger YouTube",
  "version": "1.1",
  "description": "Une extension Chrome pour télécharger des vidéos YouTube.",
  "permissions": [
    "tabs",
    "activeTab",
    "downloads"
  ],
  "host_permissions": [
    "http://localhost:5000/*"
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
